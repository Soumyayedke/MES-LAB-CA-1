ORG 0000H

; Load first value (100 decimal)
MOV A, #64H        ; A ← 100

; Load multiplier (11 decimal)
MOV B, #0BH        ; B ← 11

; Perform multiplication
MUL AB             ; Result → B:A (1100 = 044CH)

; Add extra value (6)
ADD A, #06H        ; Add 6 to lower byte

; Check for carry and adjust higher byte
JNC SKIP_INC
INC B

SKIP_INC:
; Final output:
; B:A = 0452H → 1106 decimal

STOP: SJMP STOP   ; Infinite loop to end program

END
